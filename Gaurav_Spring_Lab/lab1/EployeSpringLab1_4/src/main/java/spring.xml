<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">
	<bean id="sbu" class="com.capg.SpringLab13.SBU">
		<property name="sbuCode" value="3999"></property>
		<property name="sbuName" value="Inspiring always"></property>
		<property name="sbuHead" value="Mannat"></property>
		<property name="emplist">
			<list>
				<bean class="com.capg.SpringLab13.Employee">
					<property name="employeeId" value="12345"></property>
					<property name="employeeName" value="Satvik"></property>
					<property name="salary" value="250000"></property>
				</bean>
				<bean class="com.capg.SpringLab13.Employee">
					<property name="employeeId" value="67890"></property>
					<property name="employeeName" value="Kiran"></property>
					<property name="salary" value="26000"></property>
				</bean>
			</list>
		</property>
	</bean>

</beans>
